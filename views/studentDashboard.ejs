<%- include('header'); -%>
    <table class="table">
        <tr>
            <th>Lecture-Id</th>
            <th>Lecture Time Stamp</th>
            <th>Batch Name</th>
            <th>Batch Year</th>
        </tr>
        <% fStudent.lecturesAttended.forEach(function(lecture){  %>
            <tr>
                <td><a href = "/teacher/<%= lecture.id %>/lecture"><%= lecture.id %></a></td>
                <td><%= lecture.date %></td>
                <td><%= lecture.batchName %></td>
                <td><%= lecture.batchYear %></td>
            </tr>
        <% }) %>
    </table>
<%- include('footer'); -%>